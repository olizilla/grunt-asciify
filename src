"use strict";module.exports=function(t){t.initConfig({jshint:{all:["Gruntfile.js","tasks/*.js","<%= nodeunit.tests %>"],options:{jshintrc:".jshintrc"}},clean:{tests:["tmp"]},asciify:{default_options:{text:"Asciify",dest:"tmp/default_options"},custom_options:{text:"GRUNT!",options:{font:"doom"},dest:"tmp/custom_options"},banner:{text:"GRUNT-ASCIIFY!",options:{font:"graffiti",log:!0}}},nodeunit:{tests:["test/*_test.js"]},uglify:{src:"Gruntfile.js",dest:"Grunfile.withbanner.min.js"}}),t.loadTasks("tasks"),t.loadNpmTasks("grunt-contrib-jshint"),t.loadNpmTasks("grunt-contrib-clean"),t.loadNpmTasks("grunt-contrib-nodeunit"),t.loadNpmTasks("grunt-contrib-uglify"),t.registerTask("test",["clean","asciify","nodeunit"]),t.registerTask("default",["jshint","asciify","uglify"])};